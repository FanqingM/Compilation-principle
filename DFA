digraph DFA {
	1 [label=1 color=green shape=doublecircle]
	2 [label=2 color=gray shape=circle]
	3 [label=3 color=gray shape=circle]
	4 [label=4 color=gray shape=circle]
	5 [label=5 color=gray shape=doublecircle]
	1 -> 3 [label=a]
	1 -> 2 [label=b]
	2 -> 1 [label=b]
	2 -> 4 [label=a]
	3 -> 4 [label=b]
	3 -> 5 [label=a]
	4 -> 4 [label="a,b"]
	5 -> 3 [label=a]
	5 -> 2 [label=b]
}
